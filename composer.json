{
    "name": "shrikeh/crypto",
    "type": "library",
    "description": "OOP implementations of various encryption methods for PHP. Standardises implementation across different methods (mcrypt, openssl, etc).",
    "keywords": ["encryption", "mcrypt", "openssl", "passwords"],
    "homepage": "https://github.com/shrikeh/crypto",
    "license": "MIT",
    "authors": [
        {
            "name": "Shrikeh",
            "email": "barney@shrikeh.net"
        }
    ],
    "require": {
        "php": ">=5.3.3",
        "ircmaxell/password-compat" : "dev-master"
    },
    "require-dev" : {
        "phpspec/phpspec": "^2.5"
    },
    "config": {
        "bin-dir": "bin"
    },
    "autoload": {
        "psr-4": {
            "Shrikeh\\Crypto\\": "src"
        }
    },
    "minimum-stability": "dev"
}
